{{#items}}
    {{#map}}
.Icon {
    display: block;
    text-indent: -9999px;
    white-space: nowrap;
    overflow: hidden;
    background-image: url({{{image}}});
    background-size: {{width}} {{height}};
    background-repeat: no-repeat;
}
    {{/map}}
{{/items}}

{{#items}}
.Icon--{{name}} {
    width: {{px.width}};
    height: {{px.height}};
    background-position: {{px.offset_x}} {{px.offset_y}};
}
{{/items}}
